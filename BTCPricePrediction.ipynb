   "cell_type": "code",
   "execution_count": null,
   "metadata": {
    "editable": false
   },
   "outputs": [],
   "source": [
    "train_X, train_y = create_dataset(train_data, lag)\n",
    "test_X, test_y = create_dataset(test_data, lag)"
   ]
