   "outputs": [],
   "source": [
    "from statsmodels.tsa.stattools import pacf\n",
    "pacf_value = pacf(data, nlags=20)\n",
    "lag = 0\n",
    "# collect lag values greater than 10% correlation \n",
    "for x in pacf_value:\n",
    "    if x > 0.1:\n",
    "        lag += 1\n",
    "    else:\n",
    "        break\n",
    "print('Selected look_back (or lag = ): ', lag)"
   ]
